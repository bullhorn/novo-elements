{
    "presets": [
        "stage-0",
        "es2015"
    ],
    "sourceMaps": "inline",
    "plugins": [
        "angular2-annotations",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-flow-strip-types"
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "__coverage__",
                    {
                        "ignore": [
                            "*.spec.js",
                            "TestContext.js",
                            "spec-bundle.js"
                        ]
                    }
                ]
            ]
        }
    }
}
