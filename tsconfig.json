{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitHelpers": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "baseUrl": "./src",
    "paths": {
    },
    "lib": [
      "dom",
      "es6"
    ],
    "types": [
      "node",
      "jasmine",
      "webpack"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "files": [
    "./demo/custom-typings.d.ts",
    "./novo-elements.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "forkCheckerSilent": true,
    "useWebpackText": true,
    "transpileOnly": false
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
