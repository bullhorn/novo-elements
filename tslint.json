{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "ban": [
      true, []
    ],
    "class-name": true,
    "curly": true,
    "eofline": true,
    "forin": false,
    "indent": [
      true,
      "spaces"
    ],
    "import-blacklist": [
      true,
      "rxjs",
      "rxjs/Rx"
    ],
    "import-spacing": true,
    "label-position": true,
    "new-parens": true,
    "no-angle-bracket-type-assertion": false,
    "no-any": false,
    "no-arg": true,
    "no-console": [
      true,
      "assert",
      "count",
      "debug",
      "dir",
      "dirxml",
      "group",
      "groupCollapsed",
      "groupEnd",
      "info",
      "log",
      "profile",
      "profileEnd",
      "table",
      "time",
      "timeEnd",
      "timeStamp",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty-interface": false,
    "no-eval": true,
    "no-inferrable-types": [
      false,
      "ignore-params"
    ],
    "no-internal-module": true,
    "no-magic-numbers": false,
    "no-namespace": true,
    "no-reference": true,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    // "no-use-before-declare": true,
    "no-var-keyword": true,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-catch",
      "check-finally",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "switch-default": true,
    "triple-equals": true,
    "use-isnan": true,
    "use-input-property-decorator": true,
    "use-output-property-decorator": true,
    // "use-host-property-decorator": true,
    "no-attribute-parameter-decorator": true,
    // "no-input-rename": true,
    // "no-output-rename": true,
    "no-forward-ref": false,
    "use-life-cycle-interface": true,
    "use-pipe-transform-interface": true
  }
}