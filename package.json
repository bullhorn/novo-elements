{
  "name": "novo-elements",
  "version": "1.1.10",
  "description": "Bullhorn's NOVO Element Repository for Angular 2",
  "scripts": {
    "clean": "node_modules/.bin/del coverage lib dist",
    "compile": "npm run compile:lib",
    "compile:lib": "babel src --ignore \".spec.js, testing/**\" --out-dir lib --copy-files",
    "compile:lib:watch": "babel src --ignore \".spec.js, testing/**\" --out-dir lib --copy-files --watch",
    "prepublish": "npm run clean && npm run compile",
    "postpublish": "npm run deploy:gh-pages",
    "lint": "eslint src demo",
    "start": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "test": "cross-env NODE_ENV=test node_modules/.bin/karma start",
    "test:watch": "cross-env NODE_ENV=test node_modules/.bin/karma start --single-run false --auto-watch true",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -v",
    "github-release": "conventional-github-releaser -p angular",
    "deploy:gh-pages": "npm run build:prod && ./node_modules/.bin/gh-pages -d dist",
    "preversion": "npm test",
    "version": "npm run changelog && git add -A",
    "postversion": "git push origin master && git push --tags && npm run github-release && npm publish",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "lint:save": "eslint src demo -f node_modules/galaxy-parser/formatters/eslint.js > ./coverage/eslint.json",
    "loc": "sloc src/ --format json > ./coverage/sloc.json",
    "pregalaxy": "npm run lint:save && npm run loc",
    "galaxy": "galaxy analyze"
  },
  "main": "lib/novo-elements.js",
  "keywords": [
    "angular2"
  ],
  "author": "Joshua Godi <joshuagodi@gmail.com>",
  "contributors": [
    {
      "name": "Brian Kimball",
      "email": "bkimball@bullhorn.com"
    },
    {
      "name": "Alec Sibilia",
      "email": "asibilia@bullhorn.com"
    },
    {
      "name": "Kameron Sween",
      "email": "ksween@bullhorn.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bullhorn/novo-elements.git"
  },
  "bugs": {
    "url": "https://github.com/bullhorn/novo-elements/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/bullhorn/novo-elements#readme",
  "dependencies": {
    "hint.css": "^2.3.0",
    "moment": "^2.13.0"
  },
  "peerDependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "async": "^2.0.0-rc.5",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "~6.10.1",
    "babel-core": "~6.10.4",
    "babel-eslint": "~6.0.4",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-angular2-annotations": "~5.1.0",
    "babel-plugin-transform-class-properties": "~6.10.2",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-flow-strip-types": "~6.8.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "~7.3.0",
    "babylon": "6.8.1",
    "compression-webpack-plugin": "0.3.1",
    "conventional-changelog": "1.1.0",
    "conventional-changelog-cli": "1.2.0",
    "conventional-github-releaser": "1.1.3",
    "copy-webpack-plugin": "3.0.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "del": "2.2.1",
    "del-cli": "0.2.0",
    "es6-promise-loader": "^1.0.1",
    "eslint": "2.13.1",
    "eslint-config-bullhorn": "~0.2.0",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "galaxy-parser": "^1.0.4",
    "gh-pages": "0.11.0",
    "ghooks": "^1.2.1",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.21.0",
    "jasmine": "2.4.1",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-coveralls": "^1.1.2",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-super-dots-reporter": "^0.1.0",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.7.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.8",
    "require-dir": "0.3.0",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "^3.2.0",
    "sloc": "^0.1.10",
    "source-map-loader": "0.1.5",
    "style-loader": "^0.13.1",
    "systemjs-builder": "0.15.23",
    "to-string-loader": "^1.1.4",
    "typescript": "^1.8.10",
    "underscore": "^1.8.3",
    "webpack": "1.13.1",
    "webpack-dashboard": "0.0.1",
    "webpack-dev-server": "1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0",
    "zone.js": "^0.6.21"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "galaxy": {
    "display": "Novo Elements",
    "type": "javascript",
    "goal": 80,
    "public": true,
    "threshold": 0.1,
    "locations": {
      "eslint": "/coverage/eslint.json",
      "sloc": "/coverage/sloc.json",
      "lcov": "/coverage/lcov.info"
    }
  }
}
